#This configutation is for {{ ansible_facts.nodename }}

upstream webservers {
    {% for host in groups.webservers %}   #for loop
        server {{ hostvars[host].ansible_facts.nodename }}:{{ http_port }};
    {% endfor %}
    #server 192.168.56.102:8000;
    #server 192.168.56.103:8000;
    }

server {
    listen 80;

    location / {
        proxy_pass http://webservers;
        }
}